{
  "compilerOptions": {
    "module": "ES2020",
    "target": "es2020",
    "outDir": "dist",
    "rootDir": ".",
    "strict": true,
    "noImplicitReturns": true,
    "noImplicitAny": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "sourceMap": true
  },
  "include": ["proxy", "management"],
  "references": [
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.test.json"
    }
  ]
}
